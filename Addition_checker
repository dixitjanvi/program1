#include <stdio.h>

int main() {
    int a, b;
    
    // Taking input from the user
    printf("Enter the value of a: ");
    scanf("%d", &a);
    
    printf("Enter the value of b: ");
    scanf("%d", &b);
    
    // Printing the sum of a and b
    printf("The sum of %d and %d is: %d\n", a, b, a + b);
    
    return 0;
}
